<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:dbUserInterfacWPF.ViewModels"
                    xmlns:view="clr-namespace:dbUserInterfacWPF.View">

	<DataTemplate DataType="{x:Type vm:HomeVM}">
		<view:Home />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:ClientVM}">
		<view:Clients />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:CategoryVM}">
		<view:Categories />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:ProductVM}">
		<view:Products />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:OrderVM}">
		<view:Orders />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:ReceiptVM}">
		<view:Receipts />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:RecordVM}">
		<view:Records />
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:SaleVM}">
		<view:Sales />
	</DataTemplate>

</ResourceDictionary>